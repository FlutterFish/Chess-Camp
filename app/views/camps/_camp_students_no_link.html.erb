<div>
  <div class="card">
    <div class="card-content">
      <span class="card-title">Registered Students</span>
      <% if @students.empty? %>
  		<p>No registered students at this time</p>
  	  <% else %>
        <ul>
           <% @students.each do |s| %>
            <li>
              <%= s.proper_name %>
            </li>
          <% end %>
        </ul>
      <% end %>
    </div>
  </div>
</div>
